struct location_t
{
    float x;
    float y;
    float z;
}

struct rotation_t
{
    float yaw;
    float pitch;
    float roll;
}

struct transformation_t
{
    int32_t id;
    location_t location;
    rotation_t rotation;
}

struct transformation_group
{
    int32_t num_of_actor;
    transformation_t transformation [num_of_actor]; 
}